2ï¸âƒ£ backend/app/workflows/planner.py
class Planner:
    def plan_sales_report(self, spec):
        return ["fetch_crm", "clean_data", "analyze", "generate_report", "send_email"]


3ï¸âƒ£ backend/app/workflows/executor.py
class Executor:
    def execute(self, steps):
        log = []
        for step in steps:
            log.append({"step": step, "status": "done"})
        return log


4ï¸âƒ£ backend/app/connectors/slack_connector.py
class SlackConnector:
    def send_message(self, channel, text):
        print(f"Slack: send to {channel}: {text}")
        return True


